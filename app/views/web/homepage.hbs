<!-- section -->
<div class="section">
	<!-- container -->
	<div class="container">
		<!-- row -->
		<div class="row">
			<!-- section-title -->
			<div class="col-md-12">
				<div class="section-title">
					<h2 class="title">NEARLY ENDED</h2>
					<div class="pull-right">
						<div class="product-slick-dots-1 custom-dots"></div>
					</div>
				</div>
			</div>
			<!-- /section-title -->
			<!-- Product Slick -->
			<div class="col-md-12 col-sm-6 col-xs-6">
				<div class="row">
					<div id="product-slick-1" class="product-slick">

						{{#each Pro2}}
						<!-- Product Single -->
						<div class="product product-single">
							<div class="product-thumb">
								<div class="product-label">

									<span id="time{{id}}NEARLYENDED" start_date="{{start_date}}">
										<script>
											transferDateFormat(`{{start_date}}`, `time{{id}}NEARLYENDED`);
										</script>
									</span>
									<span class="sale text-center">New</span>
								</div>
								<ul class="product-countdown" id="{{id}}NEARLYENDED" expriry_date="{{expriry_date}}">
									<script>

										countDownTimer(`{{ expriry_date }}`, `{{ id }}NEARLYENDED`);
									</script>
								</ul>

								<a href="/product/productdetail/{{id}}" class=" main-btn quick-view">Detail</a>
								<img src="/img/sp/{{id}}.jpg" alt="" width="263" height="263">
							</div>
							<div class="product-body">

								<p class="align-self-end ">
									Current Price:
								</p>

								<h3 class="product-price">{{curr_price}} K</h3>
								<div class="product-rating">
									<i class="fa fa-star"></i>
									<i class="fa fa-star"></i>
									<i class="fa fa-star"></i>
									<i class="fa fa-star"></i>
									<i class="fa fa-star-o empty"></i>
								</div>
								<h2 class="product-name"><a href="#">{{product_name}}</a></h2>
								<div class="">
									<p>
										<i class="fas fa-users fa-lg text-info"> </i> {{countBidders}} bidders
										{{#if isWinned}}
										<i class="fas fa-trophy " style="margin-left: 10px; color:#29d159;"></i>
										{{winnerName}}

										{{/if}}


									</p>
									<p>Buy now: <strong>{{imme_buy_price}} K</strong></p>
								</div>
								<div class="product-btns">

									<i class="fa fa-heart main-btn icon-btn" onclick="actOnPro(event);"
										data-pro-id="{{id}}" data-islike="false">
									</i>

									<button class="primary-btn add-to-cart"><i class="fa fa-shopping-cart"></i>&nbsp
										Bid
										now</button>
								</div>
							</div>
						</div>

						<!-- /Product Single -->
						{{/each}}


					</div>
				</div>
			</div>
			<!-- /Product Slick -->
		</div>
		<!-- /row -->

